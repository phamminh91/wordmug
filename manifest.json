{
  "name": "Wordmug",
  "description": "Learn new words for a better good",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "assets/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/",
    "https://od-api.oxforddictionaries.com/",
    "https://owlbot.info/api/v1/dictionary",
    "storage"
  ],
  "browser_action": {
    "default_icon": "assets/128.png"
  }
}
